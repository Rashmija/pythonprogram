int n=input("Enter number")
a=n;
rem=0
sum=0
rev=0
if (n%2)==0:
print("even")
else
print("odd")
for i in range(2,n/2+1)
if (a%i)==0:
print("not prime")
else :
print("prime")
while a>0:
rem=n%10
sum=sum+rem*rem*rem
rev=rev*10+rem
a=a/10
if(sum==n):
print("armstrong")
else:
print ("not armstrong")
if(rev==n):
print("palindrome")
else
print("not armstrong")
